              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">import cn.hutool.core.util.ZipUtil;
import org.apache.commons.compress.archivers.tar.TarArchiveEntry;
import org.apache.commons.compress.archivers.tar.TarArchiveInputStream;

import java.io.ByteArrayInputStream;
import java.nio.charset.StandardCharsets;

byte[] content = ZipUtil.unGzip(new ByteArrayInputStream(prev.getResponseData()));
TarArchiveInputStream tarArchiveInputStream = new TarArchiveInputStream(new ByteArrayInputStream(content));
TarArchiveEntry tarArchiveEntry;
byte[] buffer = new byte[1024 * 100];
int read;
String uuid = vars.get(&quot;uuid&quot;);
while ((tarArchiveEntry = tarArchiveInputStream.getNextTarEntry()) != null){
     if(tarArchiveEntry.getName().equals(uuid + &quot;.txt&quot;)) {
           read = tarArchiveInputStream.read(buffer);
           if(read &lt;= -1){
                throw new Exception(&quot;读取数据异常&quot;);
           }
            break;
     }
}
tarArchiveInputStream.close();
String jsonString = new String(buffer, 0, read, StandardCharsets.UTF_8);
prev.setResponseData(jsonString);
log.info(jsonString);

   



</stringProp>
